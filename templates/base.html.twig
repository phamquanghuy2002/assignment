    <body>
     <ul class="nav justify-content-center|justify-content-end">
            <h3><li class="nav-item">
                <a class="nav-link" href="{{ path('home') }}">Home</a>
            </li></h3>
            <h3><li class="nav-item">
                <a class="nav-link" href="{{ path('student_index')}}">Student</a>
            </li></h3>
            <h3><li class="nav-item">
                <a class="nav-link" href="{{ path('clase_index')}}">Class</a>
            </li>
        </ul>
        {% block body %}{% endblock %}
